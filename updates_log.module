<?php

/**
 * @file
 * Updates Log module.
 */

declare(strict_types = 1);

use Drupal\updates_log\UpdatesLog;

/**
 * Implements hook_cron().
 *
 * Log module statuses when necessary.
 */
function updates_log_cron(): void {
  $logger = \Drupal::service('updates_log.updates_logger');
  $logger->run();
}
