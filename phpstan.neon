parameters:
  bootstrapFiles:
    - scan-autoload.php
  fileExtensions:
    - php
    - module
  level: 9
  parallel:
    maximumNumberOfProcesses: 10
  paths:
    - settings.php
    - updates_log.module
  ignoreErrors:
    -
      message: '#Access to an undefined property [a-zA-Z0-9]+::\$[a-zA-Z0-9]+#'
      path: settings.php
